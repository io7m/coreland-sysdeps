#!/bin/sh

. ../../sysdep-main
if [ $? -ne 0 ]
then
  exit 111
fi

sd_tmp="sysdeps.out"
f1_tmp="_sd_dlopen.h"
f2_tmp="libs-dlopen"
echo "SYSDEPS sd-dlopen run create ${f1_tmp} ${f2_tmp}"

f1_out="${SYSDEP_OUTDIR}/${f1_tmp}"
f2_out="${SYSDEP_OUTDIR}/${f2_tmp}"
sd_out="${SYSDEP_OUTDIR}/${sd_tmp}"

./dlopen ${f1_tmp} ${f2_tmp} ${sd_tmp}
cp ${f1_tmp} ${f1_out}
cp ${f2_tmp} ${f2_out}
cat ${sd_tmp} >> ${sd_out}
