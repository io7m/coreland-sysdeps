#!/bin/sh

if [ x"$1" = x ]
then
  exit 1
fi

exec 2>/dev/null
sys="$1"
model=""

case "$sys" in
  SYSDEP_OS_DARWIN)
    model=`machine`
    ;;
  *)
    model="`sysctl hw.model | tr [A-Z] [a-z]`"
    ;;
esac

case "$model" in
  ppc750) echo 'SYSDEP_CPU_PPC_MODEL_750' ;;
  ppc7450) echo 'SYSDEP_CPU_PPC_MODEL_7450' ;;
  *) echo 'SYSDEP_CPU_MODEL_UNKNOWN' ;;
esac
